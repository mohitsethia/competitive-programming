#include<stdio.h>
#include<math.h>
#include<stdlib.h>

long long int countnumber(long long int a, long long int n){
    long long int i, r;
    for(i = 0; i < n; i++){
        while(a != 0){
            a /= 10;
            r++;
        }
    }
    return r;
}
int main(){
    long long int n, k;
    scanf("%lld%lld", &n, &k);
    long long int a[n], i, count = 0;
    for(i = 0; i < n; i++){
        scanf("%lld", &a[i]);
    }
    for(i = 0; i < n; i++){
        if(countnumber(a[i], n) >= k){
            count++;
        }
    }
    printf("%lld", n - count);
    return 0;
}

